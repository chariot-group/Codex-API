{
  "name": "project-name",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write .",
    "swagger:generate": "ts-node -r tsconfig-paths/register src/swagger-generator.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dyson:spells": "ts-node -r tsconfig-paths/register src/script/runner.ts --resource=spells --mode=dyson",
    "dyson:monsters": "ts-node -r tsconfig-paths/register src/script/runner.ts --resource=monsters --mode=dyson",
    "converter:spells": "ts-node -r tsconfig-paths/register src/script/runner.ts --resource=spells --mode=converter",
    "converter:monsters": "ts-node -r tsconfig-paths/register src/script/runner.ts --resource=monsters --mode=converter",
    "dyson:spells:prod": "node dist/src/script/runner.js --resource=spells --mode=dyson",
    "dyson:monsters:prod": "node dist/src/script/runner.js --resource=monsters --mode=dyson",
    "converter:spells:prod": "node dist/src/script/runner.js --resource=spells --mode=converter",
    "converter:monsters:prod": "node dist/src/script/runner.js --resource=monsters --mode=converter"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
